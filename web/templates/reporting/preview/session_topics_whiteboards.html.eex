<%= for session_topic <- @session_topics do %>
  <div class='session-topics-container'>
    <div class='report-topic'>
      <%= session_topic.name %>
    </div>
    <%= render "shapes.html", session_topic: session_topic, shapes: session_topic.shapes, conn: assigns[:conn], session: assigns[:session] %>
    <hr/>
  </div>
<% end %>
